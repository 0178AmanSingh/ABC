@model IEnumerable<ApiView.Models.Api>
@{
    ViewData["Title"] = "Home Page";
}




<h1>Table Api</h1>
<br /><br />


<table class="table table-hover">
    <tr class="bg-info">
        <th>Id</th>
         <th>Name</th>
    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
</table>
